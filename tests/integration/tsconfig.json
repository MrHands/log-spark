{
	"$schema": "http://json.schemastore.org/tsconfig",
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"plugins": [
			{
				"transform": "../../build",
				"logSeverityMinimum": 2,
				"logSeverityMaximum": 3
			}
		]
	},
	"include": [
		"../../types",
		"**/*.ts",
		".eslintrc.js"
	],
	"exclude": [
		"node_modules"
	]
}